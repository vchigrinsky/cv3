{
    "train_table": {
        "type": "default",
        "path": "",
        "prefix": "",
        "mode": "gray"
    },
    "train_loader": {
        "type": "default",
        "transform": [
            {
                "transform": "resize",
                "size": 32
            },
            {
                "transform": "random_crop",
                "size": 28
            },
            {
                "transform": "normalize",
                "mean": 0.1309,
                "std": 0.3084
            }
        ],
        "sampler": {
            "type": "default",
            "batch_size": 128
        },
        "num_workers": 0
    },
    "stem": {
        "type": "default",
        "out_channels": 16
    },
    "body": {
        "type": "convnet"
    },
    "neck": {
        "type": "default",
        "out_channels": 128
    },
    "head": {
        "type": "default"
    },
    "loss": {
        "type": "xent"
    },
    "optimizer": {
        "type": "sgd",
        "lr": 0.1,
        "weight_decay": 0.0005,
        "momentum": 0.9,
        "nesterov": true
    },
    "scheduler": {
        "type": "sequential",
        "epochs": 16,
        "schedulers": [
            {
                "type": "cosine",
                "fraction": 0.05,
                "start": 0.0,
                "stop": 1.0
            },
            {
                "type": "cosine",
                "fraction": 0.95,
                "start": 1.0,
                "stop": 0.0
            }
        ]
    },
    "seed": 420,
    "test_table": {
        "type": "default",
        "mode": "gray"
    },
    "test_loader": {
        "type": "default",
        "transform": [
            {
                "transform": "resize",
                "size": 32
            },
            {
                "transform": "center_crop",
                "size": 28
            },
            {
                "transform": "normalize",
                "mean": 0.1309,
                "std": 0.3084
            }
        ]
    }
